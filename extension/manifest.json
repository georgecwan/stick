{
  "name": "Stick",
  "version": "1.0",
  "description": "Take notes efficiently using Stick!",
  "permissions": ["storage", "tabs"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Stick",
    "default_icon": "images/logo.png"
  },
  "icons": {
    "128": "images/logo.png"
  }
}
